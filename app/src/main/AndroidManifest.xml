<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vegetablestrading">

    <uses-permission android:name="android.permission.INTERNET" />
    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.RECEIVE_SMS" />
    <uses-permission android:name="android.permission.READ_SMS" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.WRITE_SMS" />

    <application
        android:name=".activity.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".activity.LoginActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".wxapi.WXPayEntryActivity"
            android:exported="true"
            android:launchMode="singleTop" />
        <activity
            android:name="com.alipay.sdk.app.H5PayActivity"
            android:configChanges="orientation|keyboardHidden|navigation"
            android:exported="false"
            android:screenOrientation="behind" />
        <activity
            android:name="com.alipay.sdk.auth.AuthActivity"
            android:configChanges="orientation|keyboardHidden|navigation"
            android:exported="false"
            android:screenOrientation="behind" />
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/title_activity_login"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.RegistActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.MineMode.ActivateUserActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.MineMode.MyInfoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.MineMode.ModifyPwdActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.MineMode.AboutUsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.MineMode.MyIntegralActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.MineMode.TransportRecordActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.MineMode.TransportInfoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.MineMode.myApply.MyApplyRecordActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.MineMode.myApply.AddApplyActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.MineMode.myApply.ApplyDescriptActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.MineMode.myPick.PickRecordActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.MineMode.CustomerComplainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.MineMode.QuitPetActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ForgetPwdActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.MineMode.TermOfServiceActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.MineMode.ActivatedActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.MineMode.myPick.MyPickActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".activity.VegetableDescriptActivity"></activity>
    </application>

</manifest>