<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:orientation="vertical">

    <include
        layout="@layout/app_top_layout"
        android:layout_width="match_parent"
        android:layout_height="50dp"></include>

    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#f1f1f1"></View>

    <com.vegetablestrading.customViews.CustomView
        android:id="@+id/apply_type_descript_cv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        app:left_button_text="申请类型"
        app:left_button_text_color="@android:color/black"
        app:right_button_text="增加"
        app:right_button_text_color="@android:color/black" />
    <View
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:background="#f1f1f1"></View>
    <TextView
        android:id="@+id/apply_startTime_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:paddingBottom="10dp"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="10dp"
        android:text="申请开始时间"
        android:textColor="@android:color/black"
        android:textSize="14sp" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/app_black"></View>

    <TextView
        android:id="@+id/apply_endTime_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:paddingBottom="10dp"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="10dp"
        android:text="申请结束时间"
        android:textColor="@android:color/black"
        android:textSize="14sp" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/app_black"></View>
    <TextView
        android:id="@+id/apply_uploadTime_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:paddingBottom="10dp"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="10dp"
        android:text="申请提交时间"
        android:textColor="@android:color/black"
        android:textSize="14sp" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/app_black"></View>

    <TextView
        android:id="@+id/apply_info_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:paddingBottom="10dp"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="10dp"
        android:text="申请内容"
        android:textColor="@android:color/black"
        android:textSize="14sp" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/app_black"></View>

    <com.vegetablestrading.customViews.CustomView
        android:id="@+id/operateStatus_cv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        app:left_button_text="审核状态"
        app:left_button_text_color="@android:color/black"
        app:right_button_text="增加"
        app:right_button_text_color="@android:color/black" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/app_black"></View>

    <com.vegetablestrading.customViews.CustomView
        android:id="@+id/operatingPeople_cv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        app:left_button_text="审核人"
        app:left_button_text_color="@android:color/black"
        app:right_button_text="增加"
        app:right_button_text_color="@android:color/black" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/app_black"></View>

    <com.vegetablestrading.customViews.CustomView
        android:id="@+id/operatingTime_cv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        app:left_button_text="审核时间"
        app:left_button_text_color="@android:color/black"
        app:right_button_text="增加"
        app:right_button_text_color="@android:color/black" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/app_black"></View>

    <com.vegetablestrading.customViews.CustomView
        android:id="@+id/operatingNote_cv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        app:left_button_text="审批详情"
        app:left_button_text_color="@android:color/black"
        app:right_button_text="增加"
        app:right_button_text_color="@android:color/black" />
</LinearLayout>
