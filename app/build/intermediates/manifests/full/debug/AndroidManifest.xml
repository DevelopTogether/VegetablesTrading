<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vegetablestrading"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />
    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <!-- 用于进行网络定位 -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" /> <!-- 用于访问GPS定位 -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" /> <!-- 用于获取wifi的获取权限，wifi信息会用来进行网络定位 -->
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" /> <!-- 在SDCard中创建与删除文件权限 -->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" /> <!-- 用于申请调用A-GPS模块 -->
    <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="25.3.1" />

    <application
        android:name="com.vegetablestrading.activity.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar" >
        <activity android:name="com.vegetablestrading.activity.LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.alipay.sdk.app.H5PayActivity"
            android:configChanges="orientation|keyboardHidden|navigation"
            android:exported="false"
            android:screenOrientation="behind" />
        <activity
            android:name="com.alipay.sdk.auth.AuthActivity"
            android:configChanges="orientation|keyboardHidden|navigation"
            android:exported="false"
            android:screenOrientation="behind" />
        <activity
            android:name="com.vegetablestrading.activity.MainActivity"
            android:label="@string/title_activity_login" />
        <activity android:name="com.vegetablestrading.activity.RegistActivity" />
        <activity android:name="com.vegetablestrading.activity.MineMode.ActivateUserActivity" />
        <activity android:name="com.vegetablestrading.activity.MineMode.MyInfoActivity" />
        <activity android:name="com.vegetablestrading.activity.MineMode.ModifyPwdActivity" />
        <activity android:name="com.vegetablestrading.activity.MineMode.AboutUsActivity" />
        <activity android:name="com.vegetablestrading.activity.MineMode.MyIntegralActivity" />
        <activity android:name="com.vegetablestrading.activity.MineMode.TransportRecordActivity" />
        <activity android:name="com.vegetablestrading.activity.MineMode.TransportInfoActivity" />
        <activity android:name="com.vegetablestrading.activity.MineMode.myApply.MyApplyRecordActivity" />
        <activity android:name="com.vegetablestrading.activity.MineMode.myApply.AddApplyActivity" />
        <activity android:name="com.vegetablestrading.activity.MineMode.myApply.ApplyDescriptActivity" />
        <activity android:name="com.vegetablestrading.activity.MineMode.myPick.PickRecordActivity" />
        <activity android:name="com.vegetablestrading.activity.MineMode.CustomerComplainActivity" />
        <activity android:name="com.vegetablestrading.activity.MineMode.QuitPetActivity" />
        <activity android:name="com.vegetablestrading.activity.ForgetPwdActivity" />
        <activity android:name="com.vegetablestrading.activity.MineMode.TermOfServiceActivity" />
        <activity android:name="com.vegetablestrading.activity.MineMode.ActivatedActivity" />
        <activity android:name="com.vegetablestrading.activity.MineMode.myPick.MyPickActivity" />
        <activity
            android:name="com.choseaddrdemo.selectAddr.ChooserActivity"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>